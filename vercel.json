{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/api-docs/swagger-ui/(.*)",
      "dest": "/api-docs/swagger-ui/$1"
    },
    {
      "src": "/api-docs/swagger-ui/swagger-ui.css",
      "headers": { "Content-Type": "text/css" }
    },
    {
      "src": "/api-docs/swagger-ui/swagger-ui-bundle.js",
      "headers": { "Content-Type": "application/javascript" }
    },
    {
      "src": "/api-docs/swagger-ui/swagger-ui-standalone-preset.js",
      "headers": { "Content-Type": "application/javascript" }
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/api-docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
