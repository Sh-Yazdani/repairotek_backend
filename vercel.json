{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/api-docs/swagger-ui/(.*)",
      "headers": { "Content-Type": "text/css" }
    },
    {
      "src": "/api-docs/swagger-ui/(.*)",
      "headers": { "Content-Type": "application/javascript" }
    }
  ]
}
