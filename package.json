{
  "name": "repairotek",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "start": "cross-env ENV_MODE=production node dist/index.js",
    "dev": "cross-env ENV_MODE=develop nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "cross-env ENV_MODE=test mocha -r ts-node/register 'test/**/*.test.ts'",
    
    "execute_resource_file": "ts-node ./scripts/resourceGeneratScripts.ts resource_name",
    "execute_doc_file": "ts-node ./scripts/docGeneratScript.ts doc_name",
    "execute_model_file": "ts-node ./scripts/modelGeneratScript.ts model_name",
    "execute_validation_file": "ts-node ./scripts/validationGeneratScript.ts validation_name",
    "execute_repository_file": "ts-node ./scripts/repositoryGeneratScript.ts repository_name",
    "execute_service_file": "ts-node ./scripts/serviceGeneratScript.ts service_name",
    "execute_controller_file": "ts-node ./scripts/controllerGeneratScript.ts controller_name"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.2",
    "@types/pino-multi-stream": "^5.1.6",
    "cross-env": "^7.0.3",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.0",
    "joi": "^17.13.1",
    "mongoos": "^0.0.1-security",
    "mongoose": "^8.2.4",
    "morgan": "^1.10.0",
    "pino": "^9.1.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^11.0.0",
    "typescript-di": "^0.6.1"
  }
}
